{
    "name":"YoutubeSummariser",
    "manifest_version":2,
    "version":"1.0",
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["inject.js"],
        "run_at": "document_end" 
      }
    ],
    "web_accessible_resources": [
      "content.js"
    ]
  }